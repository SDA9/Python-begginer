row1 = ["⬜️","⬜️","⬜️"]
row2 = ["⬜️","⬜️","⬜️"]
row3 = ["⬜️","⬜️","⬜️"]
mapR = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")

vertical = int(position[0]) - 1
horizontal = int(position[1]) - 1

mapR[vertical][horizontal] = " X"

print(f"{row1}\n{row2}\n{row3}"
