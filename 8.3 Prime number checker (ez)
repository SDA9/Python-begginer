def prime_checker(number):
  is_it_prime = True
  for i in range(2,number):
    if number % i == 0:
      is_it_prime = False
  if is_it_prime:
    print("It is a prime number")
  else:
    print("It is not a prime number")    
      
    
numb = int(input("Check this number: "))
prime_checker(number=numb)

